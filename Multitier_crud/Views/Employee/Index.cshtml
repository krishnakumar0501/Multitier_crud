@model IEnumerable<CommonLayer.Models.Employee>


    <a asp-action="Create" asp-controller="Employee" class="btn">Create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Age</td>
            <td>Salary</td>
            <td>Phone</td>
            <td>Department_id</td>
            <td>Action</td>
        </tr>
    </thead>

    @foreach(var emp in Model)
    {
        <tr>
            <th>@emp.id</th>
            <th>@emp.Name</th>
            <th>@emp.Salary</th>
            <th>@emp.Age</th>
            <th>@emp.Phone</th>
            <th>@emp.Department_id</th>
            <th>
                <a asp-action="Edit" asp-controller="Employee" asp-route-id="@emp.id" class="btn" id="editbtn">Edit</a>
                <a asp-action="Delete" asp-controller="Employee" asp-route-id="@emp.id" class="btn" id="deletebtn">Delete</a>
            </th>
        </tr>
    }
</table>
